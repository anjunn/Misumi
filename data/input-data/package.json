{
  "name": "fr-ec-cnc-qa-automation",
  "description": "Automation for fr-ec-cnc",
  "version": "1.0.0",
  "scripts": {
    "test-ci": "node_modules/.bin/wdio wdio.conf.chrome.js --suite smoke",
    "webdriver-install": "node_modules/.bin/selenium-standalone install --version=3.0.1 --drivers.chrome.version=2.35 --drivers.chrome.arch=x64",
